{
    "database": "ecommerce_db",
    "tables": {
        "users": {
            "columns": [
                {"name": "id", "type": "INTEGER", "not_null": true, "default_value": null, "primary_key": true, "auto_increment": true},
                {"name": "age", "type": "INTEGER", "not_null": true, "default_value": null, "primary_key": false},
                {"name": "email", "type": "TEXT", "not_null": true, "default_value": null, "primary_key": false, "unique": true},
                {"name": "created_at", "type": "DATETIME", "not_null": true, "default_value": "CURRENT_TIMESTAMP", "primary_key": false}
            ]
        },
        "orders": {
            "columns": [
                {"name": "order_id", "type": "INTEGER", "not_null": true, "default_value": null, "primary_key": true, "auto_increment": true},
                {"name": "user_id", "type": "INTEGER", "not_null": true, "default_value": null, "primary_key": false, "foreign_key": "users(id)"},
                {"name": "total_price", "type": "REAL", "not_null": true, "default_value": null, "primary_key": false},
                {"name": "order_date", "type": "DATETIME", "not_null": true, "default_value": "CURRENT_TIMESTAMP", "primary_key": false}
            ]
        }
    }
}
